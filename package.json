{
    "name": "@vancura/sdg-inspect",
    "author": "Vaclav Vancura (https://github.com/vancura)",
    "description": "A Vite-powered website with optimized assets and comprehensive build tooling",
    "version": "0.1.0",
    "type": "module",
    "license": "UNLICENSED",
    "scripts": {
        "dev": "cross-env NODE_ENV=development vite",
        "start": "cross-env NODE_ENV=development vite",
        "prebuild": "rm -rf dist",
        "build": "cross-env NODE_ENV=production tsc && vite build",
        "preview": "cross-env NODE_ENV=production vite preview",
        "clean": "rm -rf dist dist-ssr .stats logs",
        "clean:all": "rm -rf dist dist-ssr .stats logs node_modules .pnp.* .env .env.* .yarn/cache .yarn/unplugged .yarn/build-state.yml && yarn cache clean",
        "format": "prettier --write \"**/*.{js,ts,json,yml,yaml,md,html}\"",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix && prettier --write",
        "check": "yarn lint && yarn format --check && tsc --noEmit",
        "analyze": "rm -rf dist && cross-env NODE_ENV=production vite build --mode analyze",
        "typecheck": "tsc --noEmit",
        "setup": "yarn install && yarn dlx @yarnpkg/sdks vscode"
    },
    "dependencies": {
        "@iconify-json/solar": "^1.2.2",
        "@iconify/react": "^5.2.1",
        "@nanostores/react": "^1.0.0",
        "nanostores": "^1.0.1",
        "react": "^19.1.0",
        "react-dom": "^19.1.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.24.0",
        "@types/eslint-plugin-tailwindcss": "^3.17.0",
        "@types/node": "^20.17.30",
        "@types/react": "^19.1.2",
        "@types/react-dom": "^19.1.2",
        "@types/rollup-plugin-visualizer": "^5.0.3",
        "@typescript-eslint/eslint-plugin": "^8.30.1",
        "@typescript-eslint/parser": "^8.30.1",
        "autoprefixer": "^10.4.21",
        "cross-env": "^7.0.3",
        "eslint": "^9.24.0",
        "eslint-config-prettier": "^10.1.2",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-jsdoc": "^50.6.9",
        "eslint-plugin-n": "^17.17.0",
        "eslint-plugin-optimize-regex": "^1.2.1",
        "eslint-plugin-perfectionist": "^4.11.0",
        "eslint-plugin-promise": "^7.2.1",
        "eslint-plugin-security": "^3.0.1",
        "eslint-plugin-sonarjs": "^3.0.2",
        "eslint-plugin-tailwindcss": "^3.18.0",
        "postcss": "^8.5.3",
        "prettier": "^3.5.3",
        "prettier-plugin-jsdoc": "^1.3.2",
        "prettier-plugin-organize-attributes": "^1.0.0",
        "prettier-plugin-organize-imports": "^4.1.0",
        "prettier-plugin-tailwindcss": "^0.6.11",
        "rollup-plugin-visualizer": "^5.14.0",
        "sharp": "^0.34.1",
        "svgo": "^3.3.2",
        "tailwindcss": "^3.4.17",
        "terser": "^5.39.0",
        "typescript": "^5.8.3",
        "vite": "^6.3.0",
        "vite-imagetools": "^7.0.5",
        "vite-plugin-compression": "^0.5.1",
        "vite-plugin-fonts": "^0.7.0",
        "vite-plugin-html": "^3.2.2",
        "vite-plugin-image-optimizer": "^1.1.8",
        "vite-plugin-inspect": "^11.0.0",
        "vite-plugin-preload": "^0.4.2",
        "yaml-eslint-parser": "^1.3.0"
    },
    "packageManager": "yarn@4.6.0"
}
